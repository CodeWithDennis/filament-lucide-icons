{
    "name": "codewithdennis/filament-lucide-icons",
    "description": "A Filament plugin that integrates Lucide icons, allowing you to use them seamlessly across Filament forms, tables, actions, and more.",
    "keywords": [
        "filamentphp",
        "laravel",
        "lucide",
        "icons"
    ],
    "homepage": "https://github.com/codewithdennis/filament-lucide-icons",
    "support": {
        "issues": "https://github.com/codewithdennis/filament-lucide-icons/issues",
        "source": "https://github.com/codewithdennis/filament-lucide-icons"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "CodeWithDennis",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.22",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "CodeWithDennis\\FilamentLucideIcons\\": "src/",
            "CodeWithDennis\\FilamentLucideIcons\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeWithDennis\\FilamentLucideIcons\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeWithDennis\\FilamentLucideIcons\\FilamentLucideIconsServiceProvider"
            ],
            "aliases": {
                "FilamentLucideIcons": "CodeWithDennis\\FilamentLucideIcons\\Facades\\FilamentLucideIcons"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
